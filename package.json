    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "build-editor": "ng build editor",
    "npm-publish": "npm run build-editor && bash npm-publish",
    "copy:readme": "copy -rf README.md projects/editor/"
