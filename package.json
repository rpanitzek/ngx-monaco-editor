{
  "name": "@rolandp_86/ngx-monaco-editor",
  "version": "20.3.4",
  "description": "Monaco Code Editor For Angular 20",
  "keywords": [
    "angular 20",
    "monaco",
    "monaco-editor",
    "code",
    "editor",
    "reusable",
    "component"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "run-s build-editor serve",
    "serve": "ng serve",
    "build": "ng build --project=editor-tester --base-href=editor-tester --configuration=development",
    "build:prod": "ng build --project=editor-tester --base-href=editor-tester",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "build-editor": "ng build editor",
    "npm-publish": "npm run build-editor && bash npm-publish",
    "copy:readme": "copy -rf README.md projects/editor/",
    "update:angular": "ncu --filter \"@angular*\" -u"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/rpanitzek/ngx-monaco-editor"
  },
  "bugs": {
    "url": "https://github.com/rpanitzek/ngx-monaco-editor/issues"
  },
  "author": "Atul Kumar",
  "contributors": [
    "Atul Kumar <atulaggarwal4288@gmail.com>",
    "Roland Panitzek"
  ],
  "dependencies": {
    "@angular/animations": "~20.3.3",
    "@angular/common": "~20.3.3",
    "@angular/compiler": "~20.3.3",
    "@angular/core": "~20.3.3",
    "@angular/forms": "~20.3.3",
    "@angular/platform-browser": "~20.3.3",
    "@angular/platform-browser-dynamic": "~20.3.3",
    "@angular/router": "~20.3.3",
    "monaco-editor": "^0.54.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^20.3.0",
    "@angular-eslint/eslint-plugin": "^20.3.0",
    "@angular-eslint/eslint-plugin-template": "^20.3.0",
    "@angular-eslint/schematics": "^20.3.0",
    "@angular-eslint/template-parser": "^20.3.0",
    "@angular/build": "^20.3.4",
    "@angular/cli": "~20.3.4",
    "@angular/compiler-cli": "~20.3.3",
    "@angular/language-service": "~20.3.3",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jasmine-core": "~5.10.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^20.3.0",
    "npm-run-all": "^4.1.5",
    "typescript": "5.9.2"
  }
